{
  "name": "app-challenge",
  "version": "1.0.0",
  "description": "Fullstack Code Challenge",
  "author": "Alexander Pushkin",
  "license": "MIT",
  "engines": {
    "node": ">=14.15.0 <15.0.0"
  },
  "scripts": {
    "#client": "",
    "build:client": "webpack --mode=production --config webpack.config.js",
    "start:client": "webpack serve --mode=development",
    "test:client": "jest --config frontend/jest.config.js",
    "#server": "",
    "build:server": "webpack --mode=development --config webpack.server.config.js",
    "start:server": "yarn run build:server && yarn run nodemon:server",
    "watch:server": "webpack --mode=development --config webpack.server.config.js --watch",
    "nodemon:server": "node-dev ./dist/server/index.js",
    "test:server": "jest --config backend/jest.config.js",
    "#app": "",
    "start:app": " npm-run-all --parallel build:client build:server && npm-run-all --parallel watch:server nodemon:server"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/mui-datatables": "^3.7.0",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/sequelize": "^4.28.9",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^5.0.0-alpha.15",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.3",
    "node-dev": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "source-map-loader": "^1.1.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@types/cors": "^2.8.9",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "mobx": "4.15.7",
    "mobx-react": "6.3.1",
    "mobx-react-form": "^2.0.9",
    "mui-datatables": "^3.7.3",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0"
  }
}
